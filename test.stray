const A : *;
const B : *;
const C : *;
const a : A;
const b : B;
const h : C -> C;
free 1 : A -> C; -- F
free 2 : B -> C; -- G
let left : C = hundred C h (?1 a);
let right : C = hundred C h (?2 b);

-- example 1
free 3 : C;
let left' : C = ?3;
let right' : C = h ?3;

-- iter example from Bentkamp:
-- F (\x. x) (f a) = F f a
free 4 : (A -> A) -> A -> A;
const f'' : A -> A;
let left'' : A = ?4 (\x. x) (f'' a);
let right'' : A = ?4 f'' a;
