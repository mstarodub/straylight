-- True
let n_ : (Bool -> Nat) -> (Bool -> Nat) = \y x.y x;
let n__ : (Bool -> Nat) -> (Bool -> Nat) = \y. y;
let nftest : boolnat -> (Bool -> Natural) = \y. ?0;

let ttt : (Bool -> Natural) -> (Bool -> Natural) = \y x.y x;
let ttt' : (Bool -> Natural) -> (Bool -> Natural) = \y. \x. y x;

