-- iter example from Bentkamp:
-- F (\x. x) (f a) = F f a
free 4 : (A -> A) -> A -> A;
const f'' : A -> A;
let lc'' : A = ?4 (\x. x) (f'' a);
let rc'' : A = ?4 f'' a;
